#pragma mark - Life Cycle

- (void)viewDidLoad {
    [super viewDidLoad];
    
    self.title = @"&lt;##&gt;";
    [self setNavigationBarViewWithTitle:self.title backSelector:@selector(back)];
//    self.view.backgroundColor = ;
    
    //初始化参数
    [self variableInit];
    
    //设置Views
    [self setViews];
    
    //获取数据
    [self getData];
    
}


-(void)variableInit{
    
}

#pragma mark - Data


- (void)getData{
    [self.tabBar reloadData];
    [self.pagerController reloadData];
}


#pragma mark - setViews
- (void)setViews{
    self.tabBarHeight = 44;
    self.tabBar.layout.normalTextColor = kBlackColor;
    self.tabBar.layout.normalTextFont = kSystemFontSize(16);
    self.tabBar.layout.selectedTextColor = kGreenColor;
    self.tabBar.layout.selectedTextFont = kSystemFontSize(18);
    self.tabBar.layout.barStyle = TYPagerBarStyleNoneView;
    //    self.tabBar.layout.cellWidth = kScreenWidth/5;
    self.tabBar.layout.cellSpacing = 0;
    self.tabBar.layout.cellEdging = 0;
    self.tabBar.layout.adjustContentCellsCenter = YES;
    self.dataSource = self;
    self.delegate = self;
    self.pagerController.scrollView.scrollEnabled = NO;
    
//    self.tabBar.frame = CGRectMake(0, 64, kScreenWidth, 44);
//    self.pagerController.view.frame = CGRectMake(0, 108, kScreenWidth, kScreenHeight - 108);
[self.navigationBarView addTopConstraintToView:self.pagerController.view constant:0];
[self.view addHAlignConstraintToViews:@[self.tabBar,self.pagerController.view] constant:0];
[self.tabBar setHeightConstraintWithConstant:44];
//    [self.tabBar addBottomConstraintToView:self.tabBarController.view constant:0];
[self.view addBottomAlignConstraintToView:self.pagerController.view constant:44];
[self.view addBottomAlignConstraintToView:self.tabBar constant:0];
}

#pragma mark - Event Response
- (void)back{
    [self.navigationController popViewControllerAnimated:YES];
}

#pragma mark - Private Methods
#pragma mark - Public Methods


